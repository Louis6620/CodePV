   "source": [
    "import plotly.graph_objects as go\n",
    "import plotly.offline as pyo\n",
    "\n",
    "# Dữ liệu của bạn\n",
    "x = [1, 2, 3, 4, 5]\n",
    "y = [10, 8, 6, 4, 2]\n",
    "\n",
    "# Tạo đối tượng Figure\n",
    "fig = go.Figure(data=go.Bar(x=x, y=y))\n",
    "\n",
    "# Lưu đồ thị vào tệp HTML\n",
    "pyo.plot(fig, filename='bar_chart.html')\n"
   ]
  }
